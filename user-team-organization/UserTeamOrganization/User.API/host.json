{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      }
    }
  },
  "extensions": {
    "http": {
      "routePrefix": "",
      "cors": {
        "allowedOrigins": [ "*" ],
        "allowedMethods": [ "GET", "POST", "PUT", "DELETE" ],
        "allowedHeaders": [ "*" ]
      }
    }
  }
}
